    <style name="BindUserClearEditTextViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/shape_bg_white</item>
    </style>
    <style name="BindUserTextViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">#444444</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">right|center</item>
        <item name="android:paddingLeft">3dip</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="customDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/bg_transparency</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>
    <style name="customDownDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/bg_black</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="customImageTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/bg_black</item>
        <item name="android:padding">6dp</item>
    </style>
    <style name="customSearchDialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/bg_transparency</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>
    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/settingswindow_in_anim</item>
        <item name="android:windowExitAnimation">@anim/settingswindow_out_anim</item>
    </style>
    <style name="mypopwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/popshow_anim</item>
        <!-- 指定显示的动画xml  -->
        <item name="android:windowExitAnimation">@anim/pophidden_anim</item>
        <!-- 指定消失的动画xml  -->
    </style>
    <style name="padding_top_bottom">
        <item name="android:padding">6dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/seletor_btn_gray</item>
    </style>
    <style name="progressBarLarge" parent="android:Widget.ProgressBar.Large.Inverse">
        <item name="android:indeterminateDrawable">@drawable/progressbarrotate</item>
    </style>
    <style name="progressBarSmall" parent="android:Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/progressbarrotate</item>
    </style>
    <style name="titleGroupStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">75dip</item>
        <item name="android:background">@color/titleBG</item>
    </style>
</resources>